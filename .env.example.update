# ==========================================
# Environment Variables Added in Performance Optimization
# ==========================================

# Node Environment (development/production/test)
# Default: development
NODE_ENV=development

# Enable SQL Query Debugging
# Shows all database queries in console
# Default: false
DEBUG_SQL=false

# ==========================================
# Usage Instructions
# ==========================================

# Development:
NODE_ENV=development
DEBUG_SQL=true  # Enable to see all SQL queries

# Production:
NODE_ENV=production
DEBUG_SQL=false  # Disable to improve performance

